<head>
  <title>
    <%- title %>
  </title>
  <script src="./content/adicional/registrar.js"></script> 
</head>

<body class="text-center bodyImage3">  
  
  <div class="container">
    <div class="row">
      <div class="col-3"></div>   
      <form id="bodyContent" class="form-signin col-6" action="index.html">
        <label style="font-size: 50px;">🎲</label>
        <input id="txtUsuario" class="form-control" placeholder="NickName" required="" autofocus="">
        <div class="row">
        <input type="password" id="txtSenha" class="form-control col-6" placeholder="Senha" required="">
        <input type="password" id="txtConfirmarSenha" class="form-control col-6" placeholder="Repetir Senha" required="">
        </div>
        <input type="email" id="txtemail" class="form-control col-12" placeholder="E-mail" required="">
        <br />
        <a class="btn btn-lg btn-block btn-primary" onclick="registrar.registrar()"">Cadastrar</a>
        <p class=" mt-5 mb-3 text-muted"></p>
      </form>
      <div class="col-3"></div>
    </div>
  </div>
</body>